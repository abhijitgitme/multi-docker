{
    "AWSEBDockerrunVersion" : 2,
    "containerDefinitions" :[
        {
            "name": "client",
            "image": "dockerabhijitedi/multi-client",
            "hostname": "client",
            "essential": false,
            "memory": 128 
        },
        {
           "name": "server",
           "image": "dockerabhijitedi/multi-server",
           "hostname": "api",
           "essential": false,
           "memory": 128 
        },
        {
            "name": "worker",
            "image": "dockerabhijitedi/multi-worker",
            "hostname": "worker",
            "essential": false,
            "memory": 128

        },
        {
            "name": "nginx",
            "image": "dockerabhijitedi/multi-nginx",
            "hostname": "nginx",
            "essential": true,
            "portMappings": [
                {
                "hostPort": 80,
                "containerPOrt": 80
                }
            ],
            "links": ["client", "server"],
            "memory": 128 
        }   
    ]

}